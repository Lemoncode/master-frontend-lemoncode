    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@types/node": "22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.2"
  }
