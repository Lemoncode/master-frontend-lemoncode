{
  "name": "05-testing-01-react",
  "version": "1.0.0",
  "description": "Learn testing React by sample, each of the samples contains a readme.md file that indicates the purpose of the sample plus an step by step guide to reproduce it.",
  "type": "module",
  "scripts": {
    "start": "run-p -l type-check:watch start:dev",
    "start:dev": "vite --open",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "vitest -c ./config/test/config.ts"
  },
  "imports": {
    "#*": "./src/*"
  },
  "author": "Lemoncode",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.8.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vitest": "^3.1.2"
  }
}
