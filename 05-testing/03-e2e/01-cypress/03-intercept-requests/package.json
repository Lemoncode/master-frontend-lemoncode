    "@lemoncode/fonk": "^1.5.4",
    "@lemoncode/fonk-formik": "^4.0.1",
    "@lemoncode/fonk-min-number-validator": "^1.2.0",
    "@mui/icons-material": "^6.3.1",
    "@mui/material": "^6.3.1",
    "axios": "^1.7.9",
    "formik": "^2.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.1"
  },
  "devDependencies": {
    "@testing-library/cypress": "^10.0.2",
