{
  "name": "04-boilerplate-front",
  "version": "1.0.0",
  "description": "Member project",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "run-p -l type-check:watch start:dev",
    "start:dev": "vite --port 8080",
    "build": "npm run type-check && npm run clean && npm run build:prod",
    "build:prod": "vite build",
    "type-check": "tsc --noEmit --preserveWatchOutput",
    "type-check:watch": "npm run type-check -- --watch",
    "clean": "rimraf dist",
    "test": "vitest run -c ./config/test/config.ts",
    "test:watch": "vitest run -c ./config/test/config.ts"
  },
  "author": "Lemoncode",
  "license": "MIT",
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@lemoncode/fonk": "^1.5.4",
    "@lemoncode/fonk-formik": "^4.0.1",
    "@mui/material": "^5.15.12",
    "axios": "^1.6.7",
    "formik": "^2.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.2"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.63",
    "@types/react-dom": "^18.2.19",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.2.1",
    "jsdom": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.5",
    "vitest": "^1.3.1"
  }
}
